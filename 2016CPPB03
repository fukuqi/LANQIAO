#include<bits/stdc++.h>

using namespace std;

int main(void)
{
    int ans = 0;
    double num[]={1, 2, 3, 4, 5, 6, 7, 8, 9};
    do
    {
        if(num[0] + num[1]/num[2] + (num[3]*100+num[4]*10+num[5])/(num[6]*100+num[7]*10+num[8]) == 10)
            ans++;
    }
    while(next_permutation(num, num+9));
    cout << ans;
    return 0;
}
